late_carrier<-DELAY_CARRIER%>%filter(DEP_DELAY>=15)
(plategCARRIER<-nrow(late_carrier)/nrow(DELAY_CARRIER))
DELAY_WEATHER<-COflights%>%filter(WEATHER_DELAY>0)
